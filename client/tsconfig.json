{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "declaration": false,
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "sourceMap": false,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictBindCallApply": true,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "types": ["node", "@pinia/nuxt", "./nuxt.d.ts"]
  }
}
