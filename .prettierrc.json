{
  "singleQuote": true,
  "trailingComma": "none",
  "semi": true,
  "tabWidth": 2,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "endOfLine": "lf",
  "printWidth": 100,
  "vueIndentScriptAndStyle": false,
  "quoteProps": "as-needed",
  "plugins": ["prettier-plugin-organize-imports"]
}
